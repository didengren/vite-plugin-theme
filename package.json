{
  "name": "@trinapower/vite-plugin-theme",
  "version": "0.8.9",
  "private": false,
  "description": "Vite plugin for dynamically changing the theme color of the interface",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "es",
    "CHANGELOG.md",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.client.json",
    "build": "npm run clean  && tsup src/index.ts --dts --format cjs,esm && tsc -p tsconfig.client.json",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "example:dev": "npm -C example run serve",
    "example:build": "npm -C example run build",
    "prepublishOnly": "pinst --disable && npm run build",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky",
    "release": "bumpp --commit --push --tag && pnpm publish --access public",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "vite"
  ],
  "author": "Sam Xu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/didengren/vite-plugin-theme"
  },
  "bugs": {
    "url": "https://github.com/didengren/vite-plugin-theme/issues"
  },
  "homepage": "https://github.com/didengren/vite-plugin-theme/tree/master/#readme",
  "peerDependencies": {
    "vite": ">=2.0.0-beta.49"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/chalk": "^2.2.4",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/less": "^3.0.7",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vue/compiler-sfc": "^3.5.13",
    "bumpp": "^9.8.1",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "less": "^4.2.1",
    "lint-staged": "^15.2.10",
    "pinst": "^3.0.0",
    "prettier": "^3.4.0",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "5.4.11"
  },
  "dependencies": {
    "@types/node": "^22.10.0",
    "@types/tinycolor2": "^1.4.6",
    "chalk": "^4.1.2",
    "clean-css": "^5.3.3",
    "debug": "^4.3.7",
    "esbuild": "^0.24.0",
    "esbuild-plugin-alias": "^0.2.1",
    "tinycolor2": "^1.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://10.60.50.34:8081/repository/npm-snapshots/"
  },
  "engines": {
    "node": "22.11.0",
    "pnpm": "^9.14.2"
  }
}
